{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(npm install:*)",
      "Bash(npm run build:*)",
      "Bash(npm run dev:*)",
      "Bash(npx tsc:*)",
      "Bash(npm run lint)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(xcopy:*)",
      "Bash(cp:*)",
      "Bash(rm:*)",
      "Bash(timeout 30 npm run dev)",
      "Bash(npm uninstall:*)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:opennext.js.org)"
    ],
    "deny": []
  }
}